<!doctype html>
<html lang="en">
 <head>
  <meta charset="UTF-8">
  
  
 </head>
 <body>
  <canvas id="myCanvas" width="1000" height="1000"></canvas>
 <script type="text/javascript">
 var canvas =document.getElementById("myCanvas");
 var myContext=canvas.getContext("2d");
 myContext.beginPath();
 myContext.arc(700,700,200,1*Math.PI,2*Math.PI);
 myContext.lineWidth=20;
 myContext.strokeStyle="red";
 myContext.stroke();

 myContext.beginPath();
 myContext.arc(700,700,170,1*Math.PI,2*Math.PI);
 myContext.lineWidth=20;
 myContext.strokeStyle="orange";
 myContext.stroke();

 myContext.beginPath();
 myContext.arc(700,700,140,1*Math.PI,2*Math.PI);
 myContext.lineWidth=20;
 myContext.strokeStyle="yellow";
 myContext.stroke();

 myContext.beginPath();
 myContext.arc(700,700,130,1*Math.PI,2*Math.PI);
 myContext.lineWidth=20;
 myContext.strokeStyle="green";
 myContext.stroke();

 myContext.beginPath();
 myContext.arc(700,700,100,1*Math.PI,2*Math.PI);
 myContext.lineWidth=20;
 myContext.strokeStyle="skyblue";
 myContext.stroke();

 myContext.beginPath();
 myContext.arc(700,700,70,1*Math.PI,2*Math.PI);
 myContext.lineWidth=20;
 myContext.strokeStyle="blue";
 myContext.stroke();

 myContext.beginPath();
 myContext.arc(700,700,40,1*Math.PI,2*Math.PI);
 myContext.lineWidth=20;
 myContext.strokeStyle="purple";
 myContext.stroke();
 </script>
 </body>
</html>
